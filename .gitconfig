[user]
    email = chenfu.xie@bytedance.com
    name = chenfu.xie
[core]
    whitespace= fix,-indent-with-non-tab,trailing-space,cr-at-eol
    ignorecase= false
    editor = vim
[diff]
    tool = meld
[difftool]
    prompt = false
[difftool "meld"]
    trustExitCode = true
    cmd = open -W -a Meld --args \"$LOCAL\" \"$REMOTE\"
[merge]
    tool = meld
[mergetool]
  prompt = false
[mergetool "meld"]
    trustExitCode = true
    cmd = open -W -a Meld --args --auto-merge \"$LOCAL\" \"$BASE\" \"$REMOTE\" --output=\"$MERGED\"
[color]
        ui = auto
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
        whitespace = red reverse
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[apply]
        whitespace = fix
[alias]
        st = status
        ci = commit
        cia = commit --amend
        che = cherry-pick
        br = branch
        co = checkout
        di = diff
        fe = fetch --all
        lg = log -p
        lga = log -p --all
        lol = log --graph --decorate --pretty=oneline --abbrev-commit
        lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
        loa = log --graph --decorate --abbrev-commit --all
        ls = ls-files
        rh = reset --hard
